!!!5
%html
  %head
    %title Adnetwork
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    = render 'layouts/topbar'
    #main-body.container-fluid
      .row-fluid
        %h1 Listing vender_feedbacks
        
        %table
          %tr
            %th Chart
            %th
            %th
        
          - @vender_feedbacks.each do |vender_feedback|
            %tr
              %td= vender_feedback.chart
              %td= link_to 'Show', vender_feedback
              %td= link_to 'Destroy', vender_feedback, :method => :delete, :data => { :confirm => 'Are you sure?' }
        
        %br
        
        = link_to 'New Vender feedback', new_vender_feedback_path
